{
    "logs": {
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "/var/log/my-app.log",
                        "log_group_name": "/aws/ec2/prod-app-logs",
                        "log_stream_name": "{instance_id}",
                        "timestamp_format": "%Y-%m-%d %H:%M:%S",
                        "timezone": "Local"
                    }
                ]
            }
        }
    }
}